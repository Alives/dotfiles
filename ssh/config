StrictHostKeyChecking no
UserKnownHostsFile /dev/null

Host github.com
  IdentityFile ~/.ssh/github_rsa
  StrictHostKeyChecking yes
  User git

Host localhost
  ForwardAgent yes
  HostName localhost
  StrictHostKeyChecking yes
  IdentityFile ~/.ssh/id_rsa
  Port 2222

Host slab
  ForwardAgent yes
  HostName slab.mtv.corp
  StrictHostKeyChecking yes
  IdentityFile ~/.ssh/id_rsa
  Port 8103

Host sizzurp
  ForwardAgent yes
  HostName sizzurp.dls.corp
  StrictHostKeyChecking yes
  IdentityFile ~/.ssh/id_rsa
  Port 8103

Host dale
  ForwardAgent yes
  HostName mom.eyyit.com
  IdentityFile ~/.ssh/eyyit.key
  Port 222
  StrictHostKeyChecking yes
  User dale

Host server
  HostName server.eyyit.com
